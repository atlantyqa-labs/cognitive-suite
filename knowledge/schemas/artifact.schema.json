{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "urn:cognitive-suite:knowledge:artifact",
  "title": "Artifact",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "name",
    "type",
    "path"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "enum": [
        "script",
        "workflow",
        "doc",
        "schema",
        "dataset",
        "zip_patch",
        "config",
        "other"
      ]
    },
    "path": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "purpose": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}
