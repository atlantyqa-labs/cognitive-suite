{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "urn:cognitive-suite:knowledge:metric",
  "title": "Metric",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "name",
    "value"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "value": {},
    "source": {
      "type": "string"
    },
    "note": {
      "type": "string"
    },
    "ts": {
      "type": "string",
      "format": "date-time"
    }
  }
}
